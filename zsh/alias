# Aliases

#list
alias ll='ls -al'

#ps
alias psa='ps aux'
alias psg='ps aux | grep'

#kill
alias k9='kill -9'

#mercurial
alias hs='hg status'
alias hf='hg fetch'
alias hp='hg push'
alias hfp='hg fetch && hg push'
alias hc='hg commit -m'

# Sophia
alias _ps='pg_ctl -D /usr/local/var/postgres -l logfile start'

take () {
	mkdir $1
	cd $1
}
